# Production environment variables for Docker
NODE_ENV=production
PORT=3000

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_TTL=900

# Groq API (required for LLM functionality)
# Get your API key at: https://console.groq.com
GROQ_API_KEY=your_groq_api_key_here

# HuggingFace (required for embeddings)
# Get your API key at: https://huggingface.co/settings/tokens
HUGGINGFACE_API_KEY=your_huggingface_api_key_here

# Logging
LOG_LEVEL=info
